{
	"compilerOptions": {
		"module": "es2022",
		"target": "es2022",
		"rootDir": "./app",
		"outDir": "./dist",

		/* Stricts */
		"strict": true,
		"noImplicitAny": false,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictBindCallApply": true,
		"strictPropertyInitialization": true,
		"noImplicitThis": true
	}
}